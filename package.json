{
  "name": "fe-components",
  "version": "1.0.0",
  "description": "node.js testisovellus",
  "main": "index.js",
  "scripts": {
    "build-sass-dev": "node-sass public/sass/main.scss public/css/style.css",
    "watch-sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "compile-sass-prod": "node-sass public/sass/main.scss public/css/style-prod.css",
    "concat-css": "concat public/css/style-prod.css -o public/css/style-concat.css ",
    "prefix-css": "postcss --use autoprefixer -b \"last 5 versions\" public/css/style-concat.css -o public/css/style-prefix.css",
    "compress-css": "node-sass public/css/style-prefix.css public/css/style.css --output-style compressed",
    "build-sass-prod": "npm-run-all compile-sass-prod concat-css prefix-css compress-css",
    "build-wp-dev": "webpack",
    "watch-wp": "webpack -w",
    "build-wp-prod": "webpack -p --env production",
    "build-all-dev": "npm-run-all build-wp-dev build-sass-dev",
    "build-all-prod": "npm-run-all build-wp-prod build-sass-prod",
    "dev-server": "webpack-dev-server",
    "test": "jest --config=jest.config.json",
    "start": "node server/server.js"
  },
  "author": "Kaj Risberg",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concat": "^1.0.3",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "enzyme-to-json": "^3.0.1",
    "jest": "^20.0.4",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "raf": "^3.3.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {}
}
